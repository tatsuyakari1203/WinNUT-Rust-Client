{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "WinNUT Rust Client",
  "version": "0.1.0",
  "identifier": "com.admin.ups-client",
  "build": {
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "WinNUT Rust Client",
        "width": 1100,
        "height": 650,
        "resizable": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEYzNDJDNzcyRkRCNzczOEQKUldTTmM3Zjljc2RDODRmTmI3R1ByUnZKeDJDSXBLQlpUTW1wcXJjWWxGQTV2UkwrVm1rOFl5V3AK",
      "endpoints": [
        "https://github.com/tatsuyakari1203/WinNUT-Rust-Client/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
